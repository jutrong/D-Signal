{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@_*": ["src/*"],
      "@_remote/*": ["src/remote/*"],
      "@_assets/*": ["src/assets/*"],
      "@_components/*": ["src/components/*"],
      "@_pages/*": ["src/pages/*"],
      "@_recoil/*": ["src/recoil/*"],
      "@_style/*": ["src/style/*"],
      "@_types/*": ["src/types/*"],
      "@_utils/*": ["src/utils/*"],
      "@_constants/*": ["src/constants/index"],
      "@_hooks/*": ["src/hooks/*"]
    }
  }
}
